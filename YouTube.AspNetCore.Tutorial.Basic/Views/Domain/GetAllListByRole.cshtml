@model List<DomainListVM>
@{
    ViewData["Title"] = "GetAllListByRole";
}

<h1>Roles Domain</h1>


<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">CONTROLLER NAME</th>          
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.ControllerName.Name</td>               
            </tr>
        }
    </tbody>
</table>

